<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<query name="getApproveTypeByName">
		<![CDATA[FROM ApproveType at WHERE at.name=:name ]]>
	</query>
	<query name="getApproveTypeList">
		<![CDATA[FROM ApproveType at WHERE at.status='Y' ]]>
	</query>
</hibernate-mapping>