<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<query name="getApproveByName">
		<![CDATA[FROM Approve a WHERE a.name=:name ]]>
	</query>
	<query name="getApproveList">
		<![CDATA[FROM Approve a WHERE a.status='Y' ]]>
	</query>
</hibernate-mapping>